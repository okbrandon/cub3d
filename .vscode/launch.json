{
	"version": "0.2.0",
	"configurations": [
	  {
		"name": "Debug cub3d",
		"type": "cppdbg",
		"request": "launch",
		"program": "${workspaceFolder}/cub3d",
		"args": ["test.cub"],
		"stopAtEntry": false,
		"cwd": "${workspaceFolder}",
		"environment": [],
		"externalConsole": false,
		"MIMode": "lldb",
		"setupCommands": [
		  {
			"description": "Enable pretty-printing for gdb",
			"text": "-enable-pretty-printing",
			"ignoreFailures": true
		  }
		]
	  }
	]
  }